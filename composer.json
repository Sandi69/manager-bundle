{
    "name": "contao/manager-bundle",
    "type": "symfony-bundle",
    "description": "Contao 4 manager bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Andreas Schempp",
            "homepage": "https://github.com/aschempp"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "~2.8|~3.0",
        "symfony/monolog-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "~2.3",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/doctrine-bundle": "~1.6",
        "contao/core-bundle": "4.3.*",
        "contao/installation-bundle": "~1.2"
    },
    "require-dev": {
        "composer/composer": "~1.0",
        "phpunit/phpunit": "~4.5",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ManagerBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ManagerBundle\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    }
}
