{
    "name": "contao/manager-bundle",
    "type": "symfony-bundle",
    "description": "Contao 4 manager bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Andreas Schempp",
            "homepage": "https://github.com/aschempp"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "^3.2",
        "symfony/monolog-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/doctrine-bundle": "^1.6",
        "contao/manager-plugin": "^1.0",
        "contao/core-bundle": "4.3.*",
        "contao/installation-bundle": "^1.2",
        "lexik/maintenance-bundle": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.0",
        "friendsofphp/php-cs-fixer": "^1.8",
        "phpunit/phpunit": "^4.5",
        "symfony/phpunit-bridge": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ManagerBundle\\Test\\": "tests/"
        }
    },
    "bin": [
        "bin/contao-console"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "contao-manager-plugin": "Contao\\ManagerBundle\\ContaoManager\\Plugin"
    }
}
